apiVersion: apps/v1
kind: ReplicaSet
metadata:
  name: rs-ngnix
  labels:
    app: nginx
    tier: frontend
spec:
  # modify replicas according to your case
  replicas: 2
  selector:
    matchLabels:
      tier: frontend
  template:
    metadata:
      labels:
        app: nginx
        tier: frontend
    spec:
      containers:
        - name: web
          image: "nginx:1.14.2"
      type: ClusterIP
  ports:
  - protocol: TCP
    po
